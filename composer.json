{
    "name": "glennraya/xendivel",
    "description": "A Laravel package for Xendit payment gateway. It supports subscriptions, debit and credit cards, and e-wallet payments and more.",
    "license": "MIT",
    "authors": [
        {
            "role": "Full-Stack Web Developer",
            "name": "Glenn Raya",
            "email": "rayaglenn@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0|^10.0",
        "illuminate/bus": "^8.0|^10.0",
        "illuminate/mail": "^8.0|^10.0",
        "illuminate/queue": "^8.0|^10.0",
        "illuminate/http": "^8.0|^10.0",
        "spatie/browsershot": "^3.60"
    },
    "autoload": {
        "psr-4": {
            "GlennRaya\\Xendivel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GlennRaya\\Xendivel\\XendivelServiceProvider"
            ]
        }
    }
}
